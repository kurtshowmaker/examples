Bootstrap: docker
From: ubuntu:18.04

%post
    apt-get update && apt-get install wget -y 

    cd /bin 
    wget https://github.com/IGBB/quack/releases/download/1.2.1/linux.quack
    chmod a+x linux.quack
    cp linux.quack quack

%runscript
    echo "Container was created $NOW"
    echo "Arguments received: $*"
    exec echo "$@"

%startscript
    linux.quack